{
  "name": "RabbitMQ",
  "category": "messaging",
  "detector_signatures": {
    "patterns": [
      "AMQP\\.",
      "Bunny\\.",
      "amqplib",
      "pika"
    ],
    "dependencies": [
      "amqp",
      "amqp-connection-manager",
      "amqplib",
      "bunny",
      "pika"
    ],
    "file_patterns": [
      "lib/**/rabbitmq/*.ex",
      "src/**/rabbitmq/*.ts",
      "src/**/rabbitmq/*.py"
    ]
  }
}
