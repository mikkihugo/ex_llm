{
  "output": {
    "filePath": "../ai-server/beam-codebase-packed.txt",
    "style": "markdown",
    "headerText": "Singularity BEAM Codebase (Elixir + Gleam)\n\nAutonomous AI agents with OTP supervision, semantic search, and living knowledge base.",
    "removeComments": false,
    "showLineNumbers": true,
    "copyToClipboard": false
  },
  "include": ["lib/**/*.ex", "lib/**/*.exs", "src/**/*.gleam"],
  "ignore": {
    "useGitignore": true,
    "useDefaultPatterns": true,
    "customPatterns": [
      "_build/**",
      "deps/**",
      ".elixir_ls/**",
      "**/*.beam",
      "test/**"
    ]
  },
  "security": {
    "enableSecurityCheck": false
  }
}
